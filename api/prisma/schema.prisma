datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String   @id @default(cuid())
  email    String   @unique
  username String   @unique
  joined   DateTime @default(now())
  password String
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt DateTime
}
